#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/chatwoot/chatwoot/node_modules/.pnpm/eslint-interactive@11.1.0_eslint@8.57.0/node_modules/eslint-interactive/bin/node_modules:/home/chatwoot/chatwoot/node_modules/.pnpm/eslint-interactive@11.1.0_eslint@8.57.0/node_modules/eslint-interactive/node_modules:/home/chatwoot/chatwoot/node_modules/.pnpm/eslint-interactive@11.1.0_eslint@8.57.0/node_modules:/home/chatwoot/chatwoot/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/chatwoot/chatwoot/node_modules/.pnpm/eslint-interactive@11.1.0_eslint@8.57.0/node_modules/eslint-interactive/bin/node_modules:/home/chatwoot/chatwoot/node_modules/.pnpm/eslint-interactive@11.1.0_eslint@8.57.0/node_modules/eslint-interactive/node_modules:/home/chatwoot/chatwoot/node_modules/.pnpm/eslint-interactive@11.1.0_eslint@8.57.0/node_modules:/home/chatwoot/chatwoot/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/eslint-interactive@11.1.0_eslint@8.57.0/node_modules/eslint-interactive/bin/eslint-interactive.js" "$@"
else
  exec node  "$basedir/../.pnpm/eslint-interactive@11.1.0_eslint@8.57.0/node_modules/eslint-interactive/bin/eslint-interactive.js" "$@"
fi
