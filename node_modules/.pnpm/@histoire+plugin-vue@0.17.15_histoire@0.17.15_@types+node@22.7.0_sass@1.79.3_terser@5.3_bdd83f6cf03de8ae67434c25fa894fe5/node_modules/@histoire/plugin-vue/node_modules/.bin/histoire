#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/chatwoot/chatwoot/node_modules/.pnpm/histoire@0.17.15_@types+node@22.7.0_sass@1.79.3_terser@5.33.0_vite@5.4.20_@types+node@22.7.0_sass@1.79.3_terser@5.33.0_/node_modules/histoire/node_modules:/home/chatwoot/chatwoot/node_modules/.pnpm/histoire@0.17.15_@types+node@22.7.0_sass@1.79.3_terser@5.33.0_vite@5.4.20_@types+node@22.7.0_sass@1.79.3_terser@5.33.0_/node_modules:/home/chatwoot/chatwoot/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/chatwoot/chatwoot/node_modules/.pnpm/histoire@0.17.15_@types+node@22.7.0_sass@1.79.3_terser@5.33.0_vite@5.4.20_@types+node@22.7.0_sass@1.79.3_terser@5.33.0_/node_modules/histoire/node_modules:/home/chatwoot/chatwoot/node_modules/.pnpm/histoire@0.17.15_@types+node@22.7.0_sass@1.79.3_terser@5.33.0_vite@5.4.20_@types+node@22.7.0_sass@1.79.3_terser@5.33.0_/node_modules:/home/chatwoot/chatwoot/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../histoire@0.17.15_@types+node@22.7.0_sass@1.79.3_terser@5.33.0_vite@5.4.20_@types+node@22.7.0_sass@1.79.3_terser@5.33.0_/node_modules/histoire/bin.mjs" "$@"
else
  exec node  "$basedir/../../../../../../histoire@0.17.15_@types+node@22.7.0_sass@1.79.3_terser@5.33.0_vite@5.4.20_@types+node@22.7.0_sass@1.79.3_terser@5.33.0_/node_modules/histoire/bin.mjs" "$@"
fi
