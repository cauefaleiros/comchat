#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/chatwoot/chatwoot/node_modules/.pnpm/vite-node@2.0.1_@types+node@22.7.0_sass@1.79.3_terser@5.33.0/node_modules/vite-node/node_modules:/home/chatwoot/chatwoot/node_modules/.pnpm/vite-node@2.0.1_@types+node@22.7.0_sass@1.79.3_terser@5.33.0/node_modules:/home/chatwoot/chatwoot/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/chatwoot/chatwoot/node_modules/.pnpm/vite-node@2.0.1_@types+node@22.7.0_sass@1.79.3_terser@5.33.0/node_modules/vite-node/node_modules:/home/chatwoot/chatwoot/node_modules/.pnpm/vite-node@2.0.1_@types+node@22.7.0_sass@1.79.3_terser@5.33.0/node_modules:/home/chatwoot/chatwoot/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../vite-node/vite-node.mjs" "$@"
else
  exec node  "$basedir/../../../vite-node/vite-node.mjs" "$@"
fi
